<footer>
    <div class="container">
        <div class="row">
                <div class="col-md-12 text-center">
                    @if (Document.ContainsKey("Canonical"))
                    {
                        <p class="text-center small text-muted">Originally published at <a href="@Document.GetString("Canonical")">@Document.GetString("Canonical")</a>.</p>
                        <br />
                    }
                    <p class="copyright text-muted m-0">@Document.GetString(WebKeys.Copyright)</p>
                    <br />
                    <ul class="list-inline text-center small">
                        @{
                            IDocument rssFeed = Outputs["**/*.rss"].FirstOrDefault();
                            IDocument atomFeed = Outputs["**/*.atom"].FirstOrDefault();
                        }
                        @if (rssFeed != null) {
                            <li class="list-inline-item">
                                <a href="@Context.GetLink(rssFeed)"><i class="fa fa-rss"></i> RSS Feed</a>
                            </li>
                        }
                        @if (atomFeed != null) {
                            <li class="list-inline-item">
                                <a href="@Context.GetLink(atomFeed)"><i class="fa fa-square-rss"></i> Atom Feed</a>
                            </li>
                        }
                        @if (Document.ContainsKey("StackOverflow")) {
                            <li class="list-inline-item">
                                <a href="http://stackoverflow.com/@Document.GetString("Stackoverflow")"><i class="fa-brands fa-stack-overflow"></i> Stack Overflow</a>
                            </li>
                        }
                        @if (Document.ContainsKey("LinkedIn")) {
                            <li class="list-inline-item">
                                <a href="https://www.linkedin.com/in/@Document.GetString("LinkedIn")"><i class="fa-brands fa-linkedin"></i> LinkedIn</a>
                            </li>
                        }
                        @if (Document.ContainsKey("GitHub")) {
                            <li class="list-inline-item">
                                <a href="https://github.com/@Document.GetString("GitHub")"><i class="fa-brands fa-github-square"></i> GitHub</a>
                            </li>
                        }
                        @if (Document.ContainsKey("Twitter")) {
                            <li class="list-inline-item">
                                <a href="https://twitter.com/@Document.GetString("Twitter")"><i class="fa-brands fa-twitter-square"></i> Twitter</a>
                            </li>
                        }
                    </ul>
                    <br />
                    <div class="font-weight-bold small"><a href="https://statiq.dev">Generated by Statiq</a></div>
                </div>
        </div>
    </div>
</footer>